
native do
	##include <LiquidCrystal.h>
	##include <watch_data.c>

	LiquidCrystal lcd(8, 9, 4, 5, 6, 7);
	WATCH_TIME_TYPE time = GET_INITIAL_WATCH_TIME();
end

_lcd.begin(16, 2);
_lcd.setCursor(0, 0);

	
var int i =0;


loop do
	_lcd.clear();
	_lcd.setCursor(0, 0);
	_lcd.print( _WATCH_TIME_TYPE_to_text(_time) );
	finalize 
		_time = _INCREMENT_WATCH_TIME(_time);
	with
	end
	await 1s;
end
